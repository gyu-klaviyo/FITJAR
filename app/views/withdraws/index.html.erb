<div class = "container">

  <h1>Listing withdraws</h1>

  <table>
    <thead>
      <tr>
        <th>Amount</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @withdraws.each do |withdraw| %>
        <tr>
          <td><%=withdraw.amount %></td>
       
        </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to 'New withdraw', new_bank_withdraw_path, data: { no_turbolink: true } %>
  <br>

</div>